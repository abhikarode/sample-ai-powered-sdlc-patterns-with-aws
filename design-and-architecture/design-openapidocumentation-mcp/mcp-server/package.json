{
  "name": "openapi-mcp-server",
  "version": "1.0.0",
  "description": "MCP server wrapper for OpenAPI documentation services",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:mcp": "node dist/index-mcp-http.js",
    "dev": "tsx watch src/index.ts",
    "dev:mcp": "tsx watch src/index-mcp-http.ts",
    "test": "jest",
    "test:mcp": "node test-mcp-jsonrpc.js",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mcp",
    "openapi",
    "documentation",
    "aws"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-bedrock-agent-runtime": "^3.844.0",
    "@aws-sdk/client-bedrock-runtime": "^3.848.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.490.0",
    "@aws-sdk/client-lambda": "^3.490.0",
    "@aws-sdk/client-s3": "^3.490.0",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@smithy/node-http-handler": "^4.1.0",
    "@types/json-schema": "^7.0.15",
    "ajv": "^8.17.1",
    "js-yaml": "^4.1.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
